}

#menuList .category{
	height:51px;
	line-height:50px;
	background:url(../images/menuIcon.png) #e6eaed 15px center no-repeat;
	border-bottom:1px solid #d0d1d3;
	color:#7c8896;
	font-size:14px;
	padding-left:40px;
	cursor:pointer;
}
#menuList .categoryList ul{
	padding-top:15px;
	display:none;
}
#menuList .categoryList li{
	height:48px;
	line-height:48px;
	cursor:pointer;
	margin-bottom:1px;
}
#menuList .categoryList li a{
	padding:0 15px;
	color:#3a444f;
	display:block;
	font-size:14px;
	position:relative;
}
#menuList .categoryList li a:hover{
	background:#e7ecf2;
}

#menuList .categoryList li>ul{
	display:none;
	padding:10px 20px;
}
#menuList .categoryList li a.checked,#menuList .categoryList li.hasChild.open{
	background:#e7ecf2;
}
#menuList .categoryList li a.checked:after{
	content:"\f00c";
	font-family:"FontAwesome";
	color:#3a444f;
	font-size:15px;
	line-height:48px;
	position:absolute;
	right:25px;
	top:0;
}
#menuList .categoryList li.hasChild{
	height:auto;
}
#menuList .categoryList li.hasChild >a{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
#menuList .categoryList li.hasChild >a:after{
	content:"\f0dd";
	font-family:"FontAwesome";
	color:#3a444f;
	font-size:20px;
	line-height:40px;
	position:absolute;
	right:25px;
	top:0;
}

#menuList .categoryList li.hasChild.open >a:after{
	content:"\f0de";
	line-height:54px;
}
#menuList .categoryList li.hasChild li{
	height:30px;
	margin-bottom:4px;
}
#menuList .categoryList li.hasChild li a{
	font-size: 12px;
	color:#3a444f;
    line-height: 30px;
    background: #d5dbe1;
    border-radius: 5px;
    padding:0 10px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
#menuList .categoryList li.hasChild li a.checked{
	font-weight:bold;
}
#menuList .categoryList li.hasChild li a.checked:after{
	color:#3a444f;
	right:8px;
	line-height:30px;
}
#menuList .open .categoryList{
	display:block;
}

/* 导航样式 */
#menuList .nav-container{
	padding-top:15px;
}
#menuList .nav-container ul{
	display:none;
}
#menuList .nav-container h3{
	color:#3a444f;
	height: 48px;
    line-height: 48px;
    cursor: pointer;
	padding:0 15px;
	position:relative;
}
#menuList .nav-container >.nav-item h3.checked,
#menuList .nav-container >.nav-item h3:hover{
	background:#e7ecf2;
}
#menuList .nav-container .hasChild > h3:after {
    content: "\f0dd";
    font-family: "FontAwesome";
    color: #3a444f;
    font-size: 16px;
    line-height: 44px;
    position: absolute;
    right: 15px;
    top: 0;
}
#menuList .nav-container .hasChild.open >h3:after {
    content: "\f0de";
    line-height: 54px;
}
#menuList .nav-container .nav-item.hasChild > ul.nav-list{
	background:#e7ecf2;
}
#menuList .nav-container > .nav-item.hasChild > ul.nav-list h3.checked,
#menuList .nav-container > .nav-item.hasChild > ul.nav-list h3:hover,
#menuList .nav-container > .nav-item ul.childList{
	background:#d5dbe1;
}
#menuList .nav-container .nav-item ul.childList h3{
	height: 35px;
    line-height: 35px;
}
#menuList .nav-container > .nav-item ul.childList h3.checked,
#menuList .nav-container > .nav-item ul.childList h3:hover{
	color:#3a444f;
	background:#c5ccd2 !important;
	font-weight:bold;
}
#menuList .nav-container h3.checked:after{
    content: "\f00c";
    font-family: "FontAwesome";
    color: #3a444f;
    font-size: 15px;
    line-height: 48px;
    position: absolute;
    right: 15px;
    top: 0;
}
#menuList .nav-container > .nav-item ul.childList h3.checked:after{
	line-height:35px;
	font-weight:normal;
}

