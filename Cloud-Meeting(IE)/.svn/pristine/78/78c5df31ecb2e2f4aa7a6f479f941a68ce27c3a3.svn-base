/*global style*/
.clearfix {
	zoom: 1;
}
.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
}
.fl {
	float: left !important;
}

.fr {
	float: right !important;
}

.txt-right {
	text-align: right;
}

.text-hide {
	overflow: hidden;
	text-indent: -1000px;
	display: block;
}

.txt-center {
	text-align: center;
}

.center {
	margin: 0 auto;
}

.no-mgr {
	margin-right: 0!important;
}
.no-pd {
	padding: 0!important;
}
.no-mgl {
	margin-left: 0!important;
}
.no-mgb{
	margin-bottom:0 !important;
}
.no-mg{
	margin:0 !important;
}
.hide {
	display: none!important;
}

.show {
	display: block!important;
}

/* loading Style */
.pageloader{display:none;height:100%;position:fixed;top:0;left:0;width:100%;z-index:9999;}
.pageloaderOverlay{width:100%;height:100%;background:#000;opacity:0.5;}
.pageloader-content{bottom:0;font-size:12px;height:120px;left:0;margin:auto;position:absolute;right:0;text-align:center;top:0;width:120px;z-index:100;}
.pageloader-spinner{position:relative;}
.pageloader .loadingAnimation{-webkit-animation:loadingAnimation 2s infinite ease-in-out;animation:loadingAnimation 2s infinite ease-in-out}
.pageloader .cube1,.pageloader .cube2,.pageloader .cube3,.pageloader .cube4{background-color:#1F8DD6;width:20px;height:20px;position:absolute;top:0;left:0}
.pageloader .cube2{-webkit-animation-delay:-.5s;animation-delay:-.5s;background-color:#FBB44C}
.pageloader .cube3{-webkit-animation-delay:-1s;animation-delay:-1s;background-color:#FC797A}
.pageloader .cube4{-webkit-animation-delay:-1.5s;animation-delay:-1.5s;background-color:#2ABB9B}
.pageloader .loading-message{margin-top:76px;position:absolute;top:0;left:0;color:#333;}
@-webkit-keyframes loadingAnimation{
	25% {-webkit-transform: translateX(42px) rotate(-90deg) scale(0.5)}
	50% {-webkit-transform: translateX(42px) translateY(42px) rotate(-180deg)}
	75% {-webkit-transform: translateX(0px) translateY(42px) rotate(-270deg) scale(0.5)}
	100% {-webkit-transform: rotate(-360deg)}
}
@keyframes loadingAnimation{
	25% {transform: translateX(42px) rotate(-90deg) scale(0.5);-webkit-transform: translateX(42px) rotate(-90deg) scale(0.5)}
	50% {transform: translateX(42px) translateY(42px) rotate(-179deg);-webkit-transform: translateX(42px) translateY(42px) rotate(-179deg)}
	50.1% {transform: translateX(42px) translateY(42px) rotate(-180deg);-webkit-transform: translateX(42px) translateY(42px) rotate(-180deg)}
	75% {transform: translateX(0px) translateY(42px) rotate(-270deg) scale(0.5);-webkit-transform: translateX(0px) translateY(42px) rotate(-270deg) scale(0.5)}
	100% {transform: rotate(-360deg);-webkit-transform: rotate(-360deg)}
}


#container{
	font-family:Tahoma, Helvetica, Arial, "微软雅黑", sans-serif;
