}
#header .sessionState{
	width:8px;
	height:8px;
	border-radius:4px;
	background:#13f71d;
	display:inline-block;
}
#header .onlineTime{
	display:inline-block;
}
#header .clearfix{
	padding-top:6px;
	padding-left:6px;
}
#header .clearfix li{
	float:left;
	background:#6b879c;
	color:#bcc6ce;
	font-size:14px;
	padding:0 30px 0 25px;
	margin-right:3px;
	height:34px;
	line-height:31px;
	border-top-left-radius:5px;
    border-top-right-radius:17px;
	position:relative;
	cursor:pointer;
}
#header .clearfix li:after{
	border-color: transparent transparent transparent #6b879c;
    border-style: solid;
    border-width: 25px 0 25px 15px;
    bottom: -25px;
    content: "";
    height: 0;
    position: absolute;
    right: -13px;
    width: 0;
}
#header .clearfix li.current:after{
	border-color: transparent transparent transparent #1587e0;
	z-index:1;
}
#header .clearfix li.current{
	color:#fff;
	background:#1587e0;
}
#container > .queryList{
