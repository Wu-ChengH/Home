	background:#148dea;
	position:relative;
}
#header #logo{
	width:300px;
	height:40px;
	background:url(../images/logo.png) no-repeat left center;
	margin-left:10px;
	float:left;
}
#header .userInfo{
	/*position:absolute;
	right:10px;
	top:0;*/
	float:right;
	height:40px;
	line-height:40px;
	color:#ccdee6;
	font-size:12px;
