.chartContaner.left{
	margin-left: 25px;
}
.chartContaner.right{
	margin-left: 10px;
}
.chartContaner{
	width:calc(50% - 30px);
	width:-moz-calc(50% - 30px);
	width:-webkit-calc(50% - 30px);
	height: 380px;
	display: inline-block;
	background-color: #fff;
	margin-top: 10px;
	border-radius: 10px;
}

/*.chartsContent{
	margin:25px 7px 27px 25px;
	overflow: hidden;
}*/
.pageContent{
    overflow: auto;
    position: absolute;
    height:100%;
    width:100%;
}
.chartBox{
	width: 100%;
	height: 350px;
}
.chartTitle {
    height: 29px;
    line-height: 29px;
    text-indent: 10px;
    font-size: 14px;
    font-weight: bold;
    border-bottom: 1px solid #e0e3e3;
    background-color: #f3f7f8;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
}
.allBtn {
    float: right;
    margin-right: 20px;
    background-color: #7b8794;
    height: 22px;
    line-height: 22px;
    color: #bdc9d0;
    border-radius: 10px;
    margin-top: 3px;
    width: 75px;
}