.noPublic{
	width:100%;
	height: 100%;
}
.noPublic #noPublicMap{
	width:100%;
	height: 100%;
}
.noPublic .noPub_right{
	height: calc(100% - 20px);
	border-radius: 5px;
	box-shadow: 0px 1px 8px #111111;
	width:500px;
	position: absolute;
	top:10px;
	right:10px;
	overflow: hidden;
}
.noPublic .layout_1{
	height: 40%;
	width: 100%;
	background-color: white;
}
.noPublic .layout_2{
	height: 60%;
	width: 100%;
	background-color: white;
}
.noPublic .layout_item .item_title{
	height: 30px;
	width:100%;
	line-height: 28px;
	text-indent: 8px;
	border-top:1px solid #CCCCCC;
	border-bottom:1px solid #CCCCCC;
	background-image: url('../img/dialog_title.png');
	background-repeat: repeat-x;
}
.noPublic .layout_item .item_container{
	height: calc(100% - 30px);
	width: 100%;
}
.noPublic .item_container_ul{
	width:100%;
	height: 100%;
}
.noPublic .item_container_ul>li{
	width:100%;
	height: 30px;
	line-height: 28px;
	border-bottom:1px solid #cccccc;
}
.noPublic .layout_item .item_header{
	background-color: #E6EBEF;
}
.noPublic .item_container_ul span{
	display: inline-block;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.noPublic .layout_item .item_name{
	width:40%;
	text-indent: 6px;
}
.noPublic .layout_item .item_type{
	width:30%;
	text-indent: 6px;
}
.noPublic .layout_item .item_capital{
	width:30%;
	text-indent: 6px;
}